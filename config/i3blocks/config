[load]
command=awk '{print $1,$2,$3}' /proc/loadavg
label=
interval=10

[temperature]
command=$HOME/.config/i3blocks-contrib/temperature/temperature
instance=dell_smm-virtual-0
label=
interval=10

[volume-pulseaudio]
command=$HOME/.config/i3blocks-contrib/volume-pulseaudio/volume-pulseaudio -F '${SYMB} ${VOL}%'
interval=once
signal=1

[battery]
command=$HOME/.config/i3blocks-contrib/battery2/battery2
markup=pango
interval=30

[time]
command=date '+%Y-%m-%d %H:%M:%S'
interval=5

